{
  "extends": "plugin:@bentley/ui",
  "parserOptions": {
    "project": "tsconfig.test.json"
  },
  "plugins": [
    "@bentley"
  ],
  "rules": {
    "@typescript-eslint/member-delimiter-style": [
      "error",
      {
        "singleline": {
          "delimiter": "semi",
          "requireLast": true
        }
      }
    ]
  }
}
