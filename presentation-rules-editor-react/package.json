    "@itwin/presentation-frontend": "^4.2.3",
    "@itwin/webgl-compatibility": "^4.2.3",
    "@testing-library/react": "^11.2.7",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.39",
    "@types/react": "^17.0.62",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "chai": "^4.3.10",
